{
  "manifest_version": 3,
  "name": "AerisTab",
  "version": "1.3.3",
  "description": "AerisTab gives your Chrome new tab a fresh, clean look with stunning backgrounds and lightning-fast search.",
  "chrome_url_overrides": {
    "newtab": "tab/index.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["locales/*.json", "resources/*.txt"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": [
    "tabs"
  ],
  "icons": {
    "360": "icons/360x360.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{d04dc3ee-ae67-488e-ac05-44d5c33a4439}"
    }
  }
}